<Window x:Class="BimPlusDemo.UserControls.InviteMember"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BimPlusDemo.UserControls"
        mc:Ignorable="d"
        Title="Invite Member" Height="265" Width="333">
    <Grid Margin="0,0,2,1">
        <Label Margin="11,10,0,141">E-mail</Label>
        <TextBox HorizontalAlignment="Left" Margin="13,34,0,0" TextWrapping="Wrap" Name="EMail" VerticalAlignment="Top" Width="305" Height="24"/>
        <Label Margin="11,60,0,116">MemberRole</Label>
        <ComboBox HorizontalAlignment="Left" Margin="11,85,0,0" VerticalAlignment="Top" Name="Role" Width="305" Height="25" />
        <Label Margin="11,115,0,60">Message:</Label>
        <TextBox Name="Message" HorizontalAlignment="Left" Margin="11,143,0,0" TextWrapping="Wrap" Text="asdf"  VerticalAlignment="Top" Height="61" Width="305"/>
        <Button Content="Invite" HorizontalAlignment="Right" Margin="0,0,21,11" VerticalAlignment="Bottom" Height="23" Width="82" Click="Invite_OnClick"/>
    </Grid>
</Window>
